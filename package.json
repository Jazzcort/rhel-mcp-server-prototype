{
  "name": "rhel-mcp-server-prototype",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "concurrently \"npm run build:mcp-app\" \"npm run build:run-shell-command-app\"",
    "build:mcp-app": "tsc --noEmit && cross-env INPUT=mcp-app.html vite build",
    "build:run-shell-command-app": "tsc --noEmit && cross-env INPUT=run-shell-command-app.html vite build",
    "watch:mcp-app": "cross-env INPUT=mcp-app.html vite build --watch",
    "watch:run-shell-command-app": "cross-env INPUT=run-shell-command-app.html vite build --watch",
    "watch": "concurrently \"npm run watch:mcp-app\" \"npm run watch:run-shell-command-app\"",
    "serve": "bun server.ts",
    "start": "cross-env NODE_ENV=development npm run build && npm run serve",
    "dev": "cross-env NODE_ENV=development concurrently 'npm run watch' 'npm run serve'"
  },
  "dependencies": {
    "@modelcontextprotocol/ext-apps": "^0.2.2",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.23",
    "bun": "^1.0.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^6.0.0",
    "vite-plugin-singlefile": "^2.3.0"
  }
}
